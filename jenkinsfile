pipeline {
    agent { docker { image 'maven:3.3.3' } }
    stages {
        stage('build') {
            steps {
                sh 'mvn --version'
                apt update -y
                apt install git -y 
                git clone https://github.com/ashishrpandey/maven-project
                cd maven-project
                mvn compile
            }
        }
    }
}
